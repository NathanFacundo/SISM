                    }
                },

                {
                    targets: 1,
                    width: 200,
                },

                {
                    targets: 4,
                    width: 200,
                },
            ],

        });


        table.on('click', '#editar', function () {
            var info = $(this).attr('data-info');
            var infoSplit = info.split('*');
            //var medico = infoSplit[0];
            var numemp = infoSplit[0];
            var regxdia = infoSplit[1];
            var fecha = infoSplit[2];


            $.ajax({
                type: 'GET',
                url: '@Url.Action("GetCita", "Laboratorio")',
                dataType: 'json',
                data: { numemp: numemp, regxdia: regxdia, fecha: fecha },
                cache: false,
                success: function (data) {
                    //alert(data.fecha);
                    $('.foto-paciente').css("background-image", "url(http://148.234.143.204/smuanl_web/fotos_dhab/" + numemp + ".jpg)");
                    $('#numempModal').val(numemp);
                    $('#regxdiaModal').val(data.regxdia);
                    $('#nombresModal').text(data.paciente);
                    $('#sexoModal').text(data.sexo);
                    $('#edadModal').text(data.edad);
                    //var turno = data.turno;
                    //var turnoSplit = turno.split(/(?=(?:..)*$)/);
                    //var hora = turnoSplit[0];
                    //var minuto = turnoSplit[1];


                    /*$("#hora option").each(function () {
                        //console.log(data[i].referido);
                        var tipo = $(this).val();
                        //console.log($(this).val());
                        //alert('fff');
                        if (hora == tipo) {
                            $(this).prop('selected', true);
                        }
                    });

                    $("#minuto option").each(function () {
                        //console.log(data[i].referido);
                        var tipo = $(this).val();
                        //console.log($(this).val());
                        //alert('fff');
                        if (minuto == tipo) {
                            $(this).prop('selected', true);
                        }
                    });*/



                    $('#telefono').val(data.telefono);
                    $('#datepicker').val(data.fecha);
                    $('#modificarModal').val(1);

                    $("#tipo option").each(function () {
                        //console.log(data[i].referido);
                        var val = data.tipo;
                        var tipo = $(this).val();
                        //console.log($(this).val());
                        //alert('fff');
                        if (val == tipo) {
                            $(this).prop('selected', true);
                        }
                    });

                    $('#derechohabientesModal').modal("show");

                },
                error: function (xhr, status, error) {
                    alert("Error");
                }
            });


        });


        table.on('click', '#confirmar', function () {

            var info = $(this).attr('data-info');
            var infoSplit = info.split('*');
            var numemp = infoSplit[0];
            var regxdia = infoSplit[1];
            var fecha = infoSplit[2];


            Swal.fire({
                icon: 'question',
                title: '¿Deseas registrar la asistencia?',
                showCancelButton: false,
                confirmButtonText: `Registrar`,
            }).then((result) => {
                /* Read more about isConfirmed, isDenied below */
                if (result.isConfirmed) {


                    $.ajax({
                        type: 'GET',
                        url: '@Url.Action("RegistrarCita", "Laboratorio")',
                        dataType: 'json',
                        data: { numemp: numemp, regxdia: regxdia, fecha: fecha },
                        cache: false,
                        success: function (data) {
                            Swal.fire({
                                icon: 'success',
                                title: data,
                            });
                        },
                        error: function (xhr, status, error) {
                            alert("Error");
                        }
                    });




                } else if (result.isDenied) {
                    Swal.fire('Changes are not saved', '', 'info')
                }
            });
        });

    });

</script>


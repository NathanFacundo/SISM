    //$('#resumenSOAP').hide();
    $('#formSoap').on('keyup', '.form-control', function () {
        if ($("#subjetivo").val() == '' || $("#subjetivo").val().length < 15 || $("#peso").val() == '' || $("#talla").val() == '' || $("#temp").val() == '' || $("#fresp1").val() == '' || $("#fcard1").val() == '' || $("#t").val() == '' || $("#a").val() == '' || $("#destrostrix").val() == '') {
            $('#siguiente1').attr('disabled', 'disabled');
            $('#siguiente1').css("background-color", "lightgray");
            $('#account').removeClass('changed');
        } else {
            $('#siguiente1').removeAttr('disabled');
            $('#siguiente1').css("background-color", "#FBC43D");
            $('#account').addClass('changed');
            var subjetivo = $("#subjetivo").val();
            $("#resumenSubjetivo").html("<p>" + subjetivo + "</p>");
        }

        if ($("#objetivo").val() == '' || $("#objetivo").val().length < 15) {
            $('#siguiente2').attr('disabled', 'disabled');
            $('#siguiente2').css("background-color", "lightgray");
            $('#personal').removeClass('changed');
        } else {
            $('#siguiente2').removeAttr('disabled');
            $('#siguiente2').css("background-color", "#FBC43D");
            $('#personal').addClass('changed');
            var objetivo = $("#objetivo").val();
            $("#resumenObjetivo").html("<p>" + objetivo + "</p>");
        }

        if ($("#plan").val() == '' || $("#plan").val().length < 15) {
            $('#confirm').removeClass('changed');
            $('#resumenSOAP').hide();
        } else {
            $('#confirm').addClass('changed');
            $('#resumenSOAP').show();
            var plan = $("#plan").val();
            $("#resumenPlan").html("<p>" + plan + "</p>");
        }
    });


</script>



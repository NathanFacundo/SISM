@model IEnumerable<UanlSISM.Models.encuesta_covid_segunda>
@{
    ViewBag.Title = "SegundaEncuestaImprimir";
}

<h2>SegundaEncuestaImprimir</h2>

<table id="example">
    <thead>
        <tr>
            <th>Expediente</th>
            <th>Detalles</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.expediente</td>
                <td><button class="btn btn-mas">Ver más</button></td>
            </tr>
        }
    </tbody>
</table>


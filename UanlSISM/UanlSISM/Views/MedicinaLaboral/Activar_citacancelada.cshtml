                <th>Médico</th>
                <th>Tipo Consulta</th>
                <th style="display: none">Estado</th>
                <th>Fecha</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                //var fecha = Convert.ToDateTime(item.start).ToString("yyyy-MM-dd");
                var edad = 0;

                if (item.fechanacimiento != null)
                {
                    edad = todayDate.Year - item.fechanacimiento.Year;
                    if (todayDate.Month < item.fechanacimiento.Month || (todayDate.Month == item.fechanacimiento.Month && todayDate.Day < item.fechanacimiento.Day))
                    {
                        edad--;

                    }
                }

                <tr>
                    <td>
                        <div data-toggle="modal" data-target="#derechohabientesModal" style="cursor:pointer" onclick="cargarDatos(this)">
                            <img src="~/Imagenes/edit_user.png" style="width:45px; margin-right:15px;">
                        </div>
                    </td>
                    <td>
                        <div>
                            @item.Nombres
                            @item.Apaterno
                            @item.Amaterno
                        </div>

                        <span><br> <b> @item.telefono</b></span>
                    </td>
                    <td>@item.Medico</td>
                    <td>@item.tipo</td>
                    <td style="display: none">@item.estatus</td>
                    <td>@item.start.ToString("yyyy-MM-dd")</td>

                    <td style="display: none;">@item.Sexo</td>
                    <td style="display: none">@edad</td>
                    <td style="display: none">@item.turno</td>
                    <td style="display: none">@item.telefono</td>
                    <td style="display: none">@item.NumEmp</td>
                    <td style="display: none">@item.Id</td>

                </tr>
            }

        </tbody>
    </table>
    <div class="col-2" style="text-align:right">
        <button id="btnVolver1" type="button" class="btn btn-warning" onclick="window.history.back();">
            Regresar
        </button>
    </div>
</div>

<div class="col-md-10 text-center m-auto p-3">
    <img src="~/Imagenes/somos-uni.png" class="somos-uni" />
</div>


<div class="modal fade" id="derechohabientesModal" tabindex="-1" role="dialog" aria-labelledby="derechohabientesModalLabel" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-body">

                <div class="row m-0">
                    <div class="col-12 p-2 m-auto text-center">

                        <input type="hidden" value="null" id="idpacientemodal" />

                        <center><div class="foto-paciente" id="fotpaciente"></div></center>

                        <h6 class="m-0 pt-3">Nombre: <b id="nombresModal"></b></h6>
                        <h6 class="m-0">Sexo: <b id="sexoModal"></b> Edad: <b id="edadModal"></b></h6>
                        <input type="hidden" id="numempModal" name="NumEmp" />
                        <input type="hidden" id="regxdiaModal" name="regxdia" />
                        <input type="hidden" id="modificarModal" name="modificar" />
                    </div>
                    <div class="col-md-12 p-2 m-auto">
                        <label for="datepicker"><b>Fecha:</b></label>
                        <input type="text" readonly id="datepicker" class="form-control" name="Fecha" placeholder="Fecha" autocomplete="off">
                    </div>
                    <div class="col-md-6 p-2 m-auto">
                        <label for="turno"><b>Turno:</b></label>
                        <input type="text" id="turno" readonly name="turno" class="form-control" placeholder="Turno" autocomplete="off">
                    </div>
                    <div class="col-md-6 p-2 m-auto">
                        <label for="tipo"><b>Tipo:</b></label>
                        <select id="tipo" name="tipo" class="form-control">
                            <option selected disabled>Con Retardo</option>
                        </select>
                    </div>
                    <div class="col-md-12 p-2 m-auto">
                        <label for="medico"><b>Medico:</b></label>
                        <select id="medico" name="Medico" class="form-control">
                            @*<option selected disabled>Médico</option>*@
                        </select>
                    </div>
                    <div class="col-md-12 p-2 m-auto">
                        <label for="telefono"><b>Teléfono:</b></label>
                        <input type="tel" id="telefono" name="telefono" class="form-control" placeholder="Teléfono" autocomplete="off">
                    </div>
                    <div class="col-12 p-2 text-center" id="botonModal">
                        <button class="btn-asignar-modal" onclick="Reactivar()">
                            Reactivar
                        </button>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>




<script>
    function cargarDatos(datos) {
        var datos = (datos.parentNode.parentNode.querySelectorAll("td"));
        var nombres = datos[1].querySelectorAll("div")[0].innerText;
        var sexo = datos[6].innerText;
        var fecha = datos[5].innerText;
        var hora = datos[8].innerText.substring(0, 2);
        var minutos = datos[8].innerText.substring(2, 4);
        var turno = hora + ":" + minutos;
        var medico = datos[2].innerText;
        var telefono = datos[9].innerText;
        var numemp = datos[10].innerText;
        var id = datos[11].innerText;

        var url = "http://148.234.143.204/smuanl_web/fotos_dhab/" + numemp + ".jpg";
        //var foto = "url('http://148.234.143.204/smuanl_web/fotos_dhab/04586101.jpg')";
        //NumEmp
        //var tipo =
        //var medico =
        //var telefono =
        console.log(url);
        $("#nombresModal").text(nombres);
        $("#sexoModal").text(sexo);
        $("#edadModal").text(datos[7].innerText);
        $("#datepicker").val(fecha);
        $("#turno").val(turno);
        $("#medico").append(" <option selected disabled>" + medico + "</option>");
        $("#telefono").val(telefono);
        $("#fotpaciente").css("background-image", "url('" + url + "')");
        $("#idpacientemodal").val(id);
    }


    function Reactivar() {
        var cod = $("#idpacientemodal").val();

        var datos = { "IdCita": cod };

        if (cod != null || cod != "null") {
           $.ajax({
                url: "@Url.Action("CambiarEstadoConsultaConRetardo", "MedicinaLaboral")",
                type: 'POST',
                dataType: 'json',
                data: datos,
              success: function (result) {
                  window.location.reload();
                },
               fail: (function () {
                   alert("Ha ocurrido un error en el servidor.");
                })
           });
        }
    }

</script>

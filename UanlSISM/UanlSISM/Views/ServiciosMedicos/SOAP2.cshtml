<script>

    $('#loadingDiv').hide();

    $('#interconsultaInfo').hide();

    $("#interCheck").change(function () {
        //Si el medicamento va a ser de resurtimiento
        if (this.checked) {
            $('#interconsultaInfo').show();

            if ($('#subjetivo').val().length > 15 && $('#objetivo').val().length > 15 && $('#plan').val().length > 15  && $('#diagnostico1').val() && $('#peso').val() && $('#talla').val()) {
                if ($('#referido').val() == null) {
                    $('#finalizar').attr("disabled", "disabled");
                    $('#finalizar').css("background-color", "lightgray");
                } else {
                    $('#finalizar').removeAttr("disabled");
                    $('#finalizar').css("background-color", "rgba(0,74,143,0.8)");
                }



            }
        } else {
            $('#interconsultaInfo').hide();
            $('#referido').prop("required", false);

            if ($('#subjetivo').val().length > 15 && $('#objetivo').val().length > 15 && $('#plan').val().length > 15 && $('#diagnostico1').val() && $('#peso').val() && $('#talla').val()) {
                $('#finalizar').removeAttr("disabled");
                $('#finalizar').css("background-color", "rgba(0,74,143,0.8)");
            }
        }
    });

    $("#referido").change(function () {
        //Si el medicamento va a ser de resurtimiento
        if ($('#subjetivo').val() && $('#objetivo').val() && $('#diagnostico1').val() && $('#plan').val() && $('#peso').val() && $('#talla').val() && $('#temp').val() && $('#fresp').val() && $('#fcard').val() && $('#t').val() && $('#a').val()) {
            $('#finalizar').removeAttr("disabled");
            $('#finalizar').css("background-color", "rgba(0,74,143,0.8)");
            $('#referido2').removeAttr('disabled');
            $('#urgenciaCheck2').removeAttr('disabled');
            $('#ref_exp2').removeAttr('disabled');
            $('#borrarInt1').css('filter', 'none');
        }
    });

    $("#borrarInt1").click(function () {
        $('#borrarInt1').css('filter', 'grayscale(100%)');
        $("#referido").val($("#referido option:first").val());
        $("#ref_exp").val('');

        if (!$('#referido2').val()) {
            $('#referido2').attr('disabled', 'disabled');
            $('#ref_exp2').attr('disabled', 'disabled');
            $('#urgenciaCheck2').attr('disabled', 'disabled');
        }

        if ($('#referido2').val()) {
            $("#referido option").each(function () {
                var val = $(this).val();
                if (val == $('#referido2').val()) {
                    $(this).prop('selected', true);
                }
            });

            //$('#referido').val($('#referido2').val());
            $('#ref_exp').val($('#ref_exp2').val());
            if ($('#urgenciaCheck2').is(':checked')) {
                $('#urgenciaCheck').prop('checked', true);
                //$('#ref_exp').val($('#ref_exp2').val());
            } else {
                $('#urgenciaCheck').prop('checked', false);
            }
            //$('#referido2').val('');
            $('#ref_exp2').val('');
            $('#urgenciaCheck2').prop('checked', false);
            //$('#referido2').attr('disabled', 'disabled');
            //$('#ref_exp2').attr('disabled', 'disabled');
            //$('#urgenciaCheck2').attr('disabled', 'disabled');

<script>
    $('#loadingDiv').show();

        $.ajax({
            url: '@Url.Action("TomarUltimoSOAP", "ServiciosMedicos")',
            dataType: "json",
            data: { numemp: $("#numemp").val() },
            success: function (data) {
                //Signos vitales
                $('#peso').val(data.peso_r);
                $('#talla').val(data.talla_r);
                $('#temp').val(data.temp_r);
                //alert(data.fresp);
                $('#fresp').val(data.fresp);
                $('#fcard').val(data.fcard);
                $('#t').val(data.ta1);
                $('#a').val(data.ta2);
                $('#destrostrix').val(data.dstx);
                $('#per_cefalico').val(data.per_cefalico);
                $('#masa_muscular').val(data.masa_muscular);
                $('#masa_grasa').val(data.masa_grasa);
                $('#porcentaje_grasa').val(data.porcentaje_grasa);

                //SOAP
                $('#subjetivo').val(data.s_exp);
                $('#objetivo').val(data.o_exp);
                $('#plan').val(data.p_exp);

                //alert(data.referido);

                //Interconsulta
                if (data.referido != null) {
                    $("#referido option").each(function () {
                        var val = $(this).val();
                        if (val == data.referido) {
                            $(this).prop('selected', true);
                            $('#interCheck').prop('checked', true);
                            $('#interconsultaInfo').show();
                            $('#ref_exp').val(data.ref_exp);
                            if (data.referido_urg == "1") {
                                $('#urgenciaCheck').prop('checked', true);
                            }
                        }
                    });

                    $('#borrarInt1').css('filter', 'none');

                } else {
                    $('#interCheck').prop('checked', false);
                    $('#interconsultaInfo').hide();
                    $('#ref_exp').val('');
                    $('#urgenciaCheck').prop('checked', false);
                    $("#referido").val($("#referido option:first").val());
                }


                if (data.referido2 != null) {
                    $("#referido2 option").each(function () {
                        var val = $(this).val();
                        if (val == data.referido2) {
                            $(this).prop('selected', true);
                            $('#ref_exp2').val(data.ref_exp2);
                            if (data.referido_urg2 == "1") {
                                $('#urgenciaCheck2').prop('checked', true);
                            }
                        }
                    });

                    $('#referido2').removeAttr('disabled');
                    $('#urgenciaCheck2').removeAttr('disabled');
                    $('#ref_exp2').removeAttr('disabled');
                    $('#borrarInt2').css('filter', 'none');


                } else {
                    if (data.referido != null) {
                        $('#referido2').removeAttr('disabled');
                        $('#urgenciaCheck2').removeAttr('disabled');
                        $('#ref_exp2').removeAttr('disabled');
                    }
                }


                if (data.referido3 != null) {
                    $("#referido3 option").each(function () {
                        var val = $(this).val();
                        if (val == data.referido3) {
                            $(this).prop('selected', true);
                            $('#ref_exp3').val(data.ref_exp3);
                            if (data.referido_urg3 == "1") {
                                $('#urgenciaCheck3').prop('checked', true);
                            }
                        }
                    });

                    $('#referido3').removeAttr('disabled');
                    $('#urgenciaCheck3').removeAttr('disabled');
                    $('#ref_exp3').removeAttr('disabled');
                    $('#borrarInt3').css('filter', 'none');


                } else {
                    if (data.referido2 != null) {
                        $('#referido3').removeAttr('disabled');
                        $('#urgenciaCheck3').removeAttr('disabled');
                        $('#ref_exp3').removeAttr('disabled');
                    }
                }


                if (data.s_exp != null) {
                    $('#subjetivoLetra').css('background-color', 'green');
                    $('#subjetivoCheck').css('background-color', 'green');
                    $('#subjetivo').css('border-bottom', '2px solid green');
                }

                if (data.o_exp != null) {
                    $('#objetivoLetra').css('background-color', 'green');
                    $('#objetivoCheck').css('background-color', 'green');
                    $('#objetivo').css('border-bottom', '2px solid green');

                }

                if (data.p_exp != null) {
                    $('#planLetra').css('background-color', 'green');
                    $('#planCheck').css('background-color', 'green');
                    $('#plan').css('border-bottom', '2px solid green');

                    if (data.s_exp != null && data.o_exp != null && data.diagnostico != null && data.peso_r != null && data.talla_r != null && data.temp_r != null && data.fresp != null && data.fcard != null && data.ta1 != null && data.ta2 != null && data.dstx != null) {
                        $('#finalizar').removeAttr("disabled");
                        $('#finalizar').css("background-color", "rgba(0,74,143,0.8)");
                    } else {
                        $('#finalizar').attr('disabled', 'disabled');
                        $('#finalizar').css('background-color', 'lightgray');
                    }

                }


                $('#diagnosticoBusquedaListaHistorial div').remove();

                if (data.diagnostico != null) {

                    $('.diagnostico-select').removeClass('diagnostico-active');

                    $('#diagnosticoBusquedaListaHistorial').append('<div class="col-md-2 p-2"><div class="shadow p-2 mb-2 rounded diagnostico-select diagnostico-active" data-clave="' + data.diagnostico + '">' + data.diagnosticoNombre + '</div></div>');
                    $('#diagnostico1').val(data.diagnosticoNombre);
                    $('#clave1').val(data.diagnostico);

                    $("#tipoconsulta1 option").each(function () {
                        var val = $(this).val();
                        if (val == data.tipconsulta) {
                            $('#tipoconsulta1').removeAttr('disabled');
                            $(this).prop('selected', true);
                        }
                    });

                    $('#borrardiag1').css('filter', 'none');
                    $('#analisisLetra').css('background-color', 'green');
                    $('#analisisCheck').css('background-color', 'green');
                    $('#analisis').css('border-bottom', '2px solid green');


                }

                if (data.diagnostico2 != null) {

                    //alert(data.diagnostico2Nombre);

                    $('#diagnosticoBusquedaListaHistorial').append('<div class="col-md-2 p-2"><div class="shadow p-2 mb-2 rounded diagnostico-select diagnostico-active" data-clave="' + data.diagnostico2 + '">' + data.diagnostico2Nombre + '</div></div>');
                    $('#diagnostico2').val(data.diagnostico2Nombre);
                    $('#clave2').val(data.diagnostico2);

                    $("#tipoconsulta2 option").each(function () {
                        var val = $(this).val();
                        if (val == data.tipconsulta2) {
                            $('#tipoconsulta2').removeAttr('disabled');
                            $(this).prop('selected', true);
                        }
                    });

                    $('#borrardiag2').css('filter', 'none');
                }

                if (data.diagnostico3 != null) {
                    $('#diagnosticoBusquedaListaHistorial').append('<div class="col-md-2 p-2"><div class="shadow p-2 mb-2 rounded diagnostico-select diagnostico-active" data-clave="' + data.diagnostico3 + '">' + data.diagnostico3Nombre + '</div></div>');

                    $("#tipoconsulta3 option").each(function () {
                        var val = $(this).val();
                        if (val == data.tipconsulta3) {
                            $('#tipoconsulta3').removeAttr('disabled');
                            $(this).prop('selected', true);
                        }
                    });

                    $('#borrardiag3').css('filter', 'none');
                }

                //alert(data.hora_termino);
                if (data.hora_termino != null) {
                    //CAMBIAR TEXTO DE BOTON DE FINALIZADO
                    $('#finalizar').text('Actualizar');
                }

                //console.log('expediente guardado');
                $('#loadingDiv').hide();

            },
            error: function (xhr, status, error) {
                /*Swal.fire({
                    position: 'center',
                    icon: 'error',
                    showConfirmButton: false,
                    timer: 10000
                });*/
            }
        });
</script>



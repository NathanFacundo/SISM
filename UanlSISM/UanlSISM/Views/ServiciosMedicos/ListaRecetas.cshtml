<script>
    $('#alertAgendar').hide();
    $("#tiempo").change(function () {
        var val = $(this).val();
        //alert(val);
        if (val == "mes(es)") {
            var cantidad = $('#cantidad').val();
            if (cantidad > 3) {
                $('#cantidad').val('3');
                $('#alertAgendar').show();
            } else {
                $('#alertAgendar').hide();
            }
        }

        if (val == "semana(s)") {
            var cantidad = $('#cantidad').val();
            if (cantidad > 12) {
                $('#cantidad').val('12');
                $('#alertAgendar').show();
            } else {
                $('#alertAgendar').hide();
            }
        }
    });
</script>
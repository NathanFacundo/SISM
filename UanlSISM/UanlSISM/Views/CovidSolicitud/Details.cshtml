<script>

    function Regresar() {

          location.href = '@Url.Action("Index", "CovidSolicitud", new { expediente = Model.NUMEMP })';
    }


    function Imprimir(id) {

        $(".folio").html("<b>Folio:</b> " + id);

        if ($("#vacunado").is(":checked") == true) {

            document.getElementById("vacunado").outerHTML = '<input class="form-check-input" onchange="vacunadoonchange()" type="checkbox"  id="vacunado" checked="true"> </input>';;

            var qvacunado = $("#qvacunado").html();
            var marcavacuna = $("#marcavacuna").val();
            var observaciones = $("#observacionesSolLab").val();

            $(".sintomaslst").append("<div class='col-6'>" + qvacunado + "<b>Marca de la vacuna: </b><p>" + marcavacuna + "</p></b><b>Observaciones: </b><p>" + observaciones + "</p></div>");

            if ($("#esquemacompleto").is(":checked") == true) {

                document.getElementById("esquemacompleto").outerHTML = '<input class="form-check-input" onchange="esquemacompletoonchange()" type="checkbox"   id="esquemacompleto" checked="true"></input>';

                var qesquemacomp = $("#qesquemacomp").html();
                var fechaultimadosis = $("#fechaultimadosis").val();

                $(".sintomaslst").append("<div class='col-6'>" + qesquemacomp + '<b>Fecha última dósis: </b><p>' + moment(fechaultimadosis).format('DD/MM/YYYY') + '</p></div>');

            }

            else {

                var qesquemacomp = $("#qesquemacomp").html();
                var fechaultimadosis = $("#fechaultimadosis").val();

                $(".sintomaslst").append("<div style='visibility:hidden' class='col-6'>" + qesquemacomp + '<b>Fecha última dósis: </b><p>' + moment(fechaultimadosis).format('DD/MM/YYYY') + '</p></div>');
            }


        }

        var lstsintomas = document.getElementsByClassName("form-check-input sintomas");
        var lstsintomaschk = [];

        for (var i = 0; i < lstsintomas.length; i++) {
            var id = lstsintomas[i].id;
            var check = $("#" + id).is(":checked");
            if (check == true) {
                lstsintomaschk.push($('#' + id).val());

            }
        }

        $(".sintomaslst").append("<br/>");
        for (var i = 0; i < lstsintomaschk.length; i++) {
            $(".sintomaslst").append("<div class='form-check form-check-inline'><input class='form-check-input' checked='true' type='checkbox' id='inlineCheckbox2'><label class='form-check-label' for='inlineCheckbox2'>" + lstsintomaschk[i] + "</label></div>");

        }

        var PCR = $("#PCR").is(":checked");
        var UER = $("#UER").is(":checked");
        var AEMA = $("#AEMA").is(":checked");

        if (PCR == true) {
            if (UER == true) {
                print2();
                return;
            }
            else {
                print();
                return;
            }
        }

        if (UER == true) {
            if (PCR == true) {
                print2();
                return;
            }
            else {
                printUER();
                return;
            }
        }

        if (AEMA == true) {
            printAEMA();
            return;
        }

    }

</script>


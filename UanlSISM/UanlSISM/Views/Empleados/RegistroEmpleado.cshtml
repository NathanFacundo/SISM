<!--Llena el ComboBox De Pais-->
<script>
    $(document).ready(function () {
        var IdPais = $("#PaisId").val();
        $.ajax({
            url: '@Url.Action("GetPais", "Empleados")',
            type: 'POST',
            dataType: 'json',
            success: function (data) {
                $.each(data, function (i, item) {
                    if (item.IdPais == IdPais) {
                        $('<option selected value="' + item.IdPais + '">' + item.NombrePais + '</option>').appendTo('#cmbPais');
                    }
                    else {
                        $('<option value="' + item.IdPais + '">' + item.NombrePais + '</option>').appendTo('#cmbPais');
                    }
                });
            },
            error: function () {
                console.log('err')
            }
        });
    });
</script>

        $.ajax({
            type: 'GET',
            url: '@Url.Action("GetEstudiosEmpleado", "Empleados")?Emp_NumEmp=' + NumEmpId,
            dataType: 'json',
            cache: false,
            success: function (data) {
                $("#tblInfoAcademica > tbody").empty();
                $.each(data, function (index, value) {
                    $("#contenido")
                        .append("<tr><td>" + value.Folio + "</td><td>" + value.Nombre + "</td><td>" + value.NivelAcademico + "</td><td>" + value.Carrera + "</td><td>" + value.Cedula + "</td><td>" + moment(value.Fecha).format('DD/MM/YYYY') + "</td><td>" + value.Escuela + "</td></tr>");
                });
            },
            error: function (xhr, status, error) {
                alert("Error");
            }
        });
    });

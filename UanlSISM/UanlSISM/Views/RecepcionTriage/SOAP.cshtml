            dataType: 'json',
            data: { NotaEvolucionT: Evolucion, ExpedientePaciente: Exp },
            success: function (data) {
                if (data.MENSAJE.substring(0, 6) == "Error:") {
                    Swal.fire({
                        icon: 'error',
                        title: 'Oops... ¡Error!',
                        text: data.MENSAJE.substring(7),
                    });
                }
                if (data.MENSAJE.substring(0, 6) == "Succe:") {
                    Swal.fire({
                        icon: 'success',
                        title: '¡Éxito!',
                        text: data.MENSAJE.substring(7),
                        willClose: () => {
                            location.reload();
                        }
                    })
                }
            },
            error: function (ex) {
                Swal.fire({
                    icon: 'error',
                    title: '¡Error!',
                });
            }
        });
